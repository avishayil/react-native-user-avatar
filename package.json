{
  "name": "react-native-user-avatar",
  "version": "1.0.6",
  "description": "React Native component for a user avatar with fallback to colored initials",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "react-native",
    "react-native-component",
    "avatar",
    "initials"
  ],
  "author": "Avishay Bar <avishay.il@gmail.com>",
  "license": "MIT",
  "repository": "github:avishayil/react-native-user-avatar",
  "dependencies": {
    "initials": "^3.0.1",
    "prop-types": "^15.7.2"
  },
  "scripts": {
    "lint": "eslint **/*.js",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "babel-jest": "24.7.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-test-renderer": "16.11.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "testMatch": [
      "**/*.test.js?(x)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/Examples"
    ]
  }
}
